                    a(class="link-topo",href="#prof",class="goesTo"  ) MIDIA
                li 
                    a(class="link-topo",href="#depoimentos", class="goesTo" ) Depoimentos
                li 
                    a(class="link-topo",href="#cuidadora", class="goesTo" ) Educadora
                li 
                    a(class="link-topo teaser",href="#teaser", class="goesTo" ) TEASER
            .fechar-menu
                img(src="img/fechar.png", alt="Botão Fechar")

        // Header navegação
        header#topo
            .container#cabeca
                .row.center-vertical
                    .logo-leort.col-xs-6.col-xs-push-3.col-md-2.col-md-push-0
                        a(href="https://www.bothanicamineral.com.br/" target="_blank")
                            img.logo(src="img/Logo-ortoshock.svg", alt="Logo Bothanica Mineral")
                    .menu-leort.col-xs-3.col-xs-pull-6.col-md-8.col-md-pull-0
                        // Navegação para desk
                        nav.visible-md.visible-lg
                            ul.display-menu
                                li 
                                    a(class="link-topo",href="#inscricao", class="goesTo") INSCRIÇÃO
                                li 
                                    a(class="link-topo",href="#agenda", class="goesTo") AGENDA
                                li 
                                    a(class="link-topo",href="#resultados", class="goesTo") Resultados
                                li 
                                    a(class="link-topo",href="#curso",  class="goesTo" )  ORTOSHOCK®
                                //li 
                                    a(class="link-topo",href="#na-midia", class="goesTo") MIDIA
                                li 
                                    a(class="link-topo",href="#depoimentos", class="goesTo" ) Depoimentos
                                li 
                                    a(class="link-topo",href="#cuidadora", class="goesTo" ) Educadora
                                li 
                                    a(class="link-topo teaser",href="#teaser", class="goesTo" ) TEASER
                        // Navegação para mob
                        nav.visible-xs.visible-sm
                                .menu-mob
                                    span.icon-bar
                                    span.icon-bar  
                                    span.icon-bar                   


                    .rede-sociais.col-xs-3.col-md-2
                        a(href="https://www.instagram.com/bothanicamineral/?hl=pt" target="_blank")
