                        a(href="https://www.facebook.com/Bothanicamineral/" target="_blank")
                            img(src="img/face.svg", alt="Facebook")
                        a(href="https://www.youtube.com/channel/UCXt09yqtrEngqUL8KANHuDA" target="_blank")
                            img(src="img/youtube.svg", alt="Youtube")

