block settings
    - var title = 'ORTOSHOCK®'
doctype
html(lang="pt")
    head
        include ../_partials/meta-tags
        include ../_partials/icons
        title #{title}
        block CSS
            link(rel="stylesheet", href="css/plugins.css")
            link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.0/slick.min.css")
            link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.0/slick-theme.min.css")
            

    body

        include ../_partials/_index/face.pug
        //#agemdaPop
            .blackPop
                .popUp.clearfix
                    img.img-responsive.block-center(src="img/agenda-placa.png", alt="")  
                    .flex-popUp  
                        .popUpItem
                            .agenda-item
                                p.data 29/05/2018
                                p.cidade Brasília/DF

                    p.infos INFORMAÇÕES E INSCRIÇÕES:
                    p.infos Concept Cursos 71 3051-1317
                    p.infos ComTato Capacitação 27 2142-5017  




                    .fechar-agenda
                        img.img-responsive.block-center(src="img/fechar-agenda.png", alt="")
        #loader
            img.loader-img.img-responsive(src="https://s3-sa-east-1.amazonaws.com/happybiz-web/BOTHANICA+MINERAL/ortoshock/lodder.png", alt="Lodder")
        #device
            .device
                img.loader-img.img-responsive(src="img/device.svg", alt="Lodder")
                p.text-inverse Use na posição vertical para melhor experiência 
        .ir-topo#ir-topo(style="display:none")
            a#ir-topos(href="#banners", class="goesTo link-topo")
                img(class="seta",src="https://s3-sa-east-1.amazonaws.com/happybiz-web/BOTHANICA+MINERAL/striort/seta.png", alt="")
        // menu-nobile
        .menu-mob-flutuante#menu-mob-flutuante.img-back-leort(style="display:none;")
            ul.display-menu-mob
                li 
                    a(class="link-topo",href="#inscricao", class="goesTo") INSCRIÇÃO
                li 
                    a(class="link-topo",href="#agenda", class="goesTo") AGENDA
                li 
                    a(class="link-topo",href="#resultados", class="goesTo") Resultados
                li 
                    a(class="link-topo goesTo",href="#curso", class="goesTo" ) ORTOSHOCK®
